    "small"
  ],
  "homepage": "https://github.com/moomoolive/small-semver",
  "bugs": {
    "url": "https://github.com/moomoolive/small-semver/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moomoolive/small-semver"
  }
}
